{
  "name": "game-of-life",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier --ignore-unknown --write",
    "format": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "npm run format"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@webgpu/types": "^0.1.40"
  }
}
